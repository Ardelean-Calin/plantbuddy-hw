{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${workspaceFolder}/lib/nrf-sdk/**"
            ],
            "forcedInclude": [
                "${workspaceFolder}/config/pb_config.h",
                "${workspaceFolder}/config/sdk_config.h",
                "${workspaceFolder}/lib/nrf-sdk/components/softdevice/s112/headers/nrf_error.h"
            ],
            "defines": [
                "APP_TIMER_V2",
                "APP_TIMER_V2_RTC1_ENABLED",
                "BOARD_PCA10040",
                "CONFIG_GPIO_AS_PINRESET",
                "DEVELOP_IN_NRF52832",
                "FLOAT_ABI_SOFT",
                "NRF52810_XXAA",
                "NRF52_PAN_74",
                "NRFX_COREDEP_DELAY_US_LOOP_CYCLES=3",
                "NRF_SD_BLE_API_VERSION=7",
                "S112",
                "SOFTDEVICE_PRESENT",
                "BL_SETTINGS_ACCESS_ONLY",
                "NRF_DFU_SVCI_ENABLED",
                "NRF_DFU_TRANSPORT_BLE=1",
                "DEBUG"
            ],
            "compilerPath": "/bin/arm-none-eabi-gcc",
            "cStandard": "gnu17",
            "cppStandard": "gnu++14",
            "intelliSenseMode": "linux-gcc-arm"
        }
    ],
    "version": 4
}