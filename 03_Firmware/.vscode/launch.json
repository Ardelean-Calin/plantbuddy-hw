{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Plantbuddy Debug",
            "cwd": "${workspaceRoot}",
            "executable": "./_build/plant-buddy.out",
            "request": "launch",
            "preLaunchTask": "Build (Debug)",
            "type": "cortex-debug",
            "servertype": "jlink",
            "serverpath": "/usr/bin/JLinkGDBServerCLExe",
            "device": "nrf52811",
            "interface": "swd",
            "svdFile": "./config/debugger/nrf52811.svd",
            "runToMain": true,
            "rttConfig": {
                "enabled": true,
                "address": "auto",
                "decoders": [
                    {
                        "label": "",
                        "port": 0,
                        "type": "console"
                    }
                ]
            }
        },
